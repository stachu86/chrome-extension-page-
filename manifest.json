{
  "manifest_version": 2,

  "name": "Page++",
  "description": "Page++ is Chrome extension that allows to increment and decrement numbers in names of internet pages.",
  "version": "1.0",
  "permissions": [ "tabs"],
  "background": {
    "persistent": false,
    "scripts": ["navigate.js"]
  },

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
 
  "commands": {
    "navigate_plus": {
      "suggested_key": { "default": "Ctrl+Right" },
      "description": "Next page"
    },
    "navigate_minus": {
      "suggested_key": { "default": "Ctrl+Left" },
      "description": "Previous page"
    } 
  }
}
